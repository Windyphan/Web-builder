{
  "functions": {
    "api/chatbot.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "api/chatbot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chatbot.py"
    },
    {
      "src": "/api/chat/status",
      "dest": "/api/chatbot.py"
    },
    {
      "src": "/",
      "dest": "/api/chatbot.py"
    }
  ]
}

